       "    .dataframe tbody tr th {\n",
       "        vertical-align: top;\n",
       "    }\n",
       "</style>\n",
       "<table border=\"1\" class=\"dataframe\">\n",
       "  <thead>\n",
       "    <tr style=\"text-align: right;\">\n",
       "      <th></th>\n",
       "      <th>Patients_Tested</th>\n",
       "      <th>practice_id</th>\n",
       "      <th>practice_name</th>\n",
       "      <th>location_id</th>\n",
       "      <th>location_name</th>\n",
       "      <th>test_date</th>\n",
       "      <th>month_year</th>\n",
       "      <th>provider</th>\n",
