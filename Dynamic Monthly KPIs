    "benchmark_all = pd.pivot_table(KPI_detail_output, index=['type'], values=['Patients_Tested'], columns=['month_year', 'test_date'], aggfunc='count', fill_value= 0, margins=True, margins_name='Subtotal Allergy Activity')\n",
